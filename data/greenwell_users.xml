	<!-- 
		IMPORT:
			Only users listed in this section will have content imported from the XML input file.
			All content from other users will be ignored.
			
			- To specify all users, enter a * wildcard
			- To list specific users, enter a CSV list of uids from the users section
			- To specify no users, leave it blank
			
			Example Scenario:  Create Communities for ONLY Dan Misawa and Frank Adams
			Example Configuration:  <communities>dmisawa,fadams</communities>
	 -->
	<import>
		<activities>*</activities>
		<blogs>*</blogs>
		<bookmarks>*</bookmarks>
		<communities>*</communities>
		<files>*</files>
		<forums>*</forums>
		<profiles>*</profiles>
		<wikis>*</wikis>
	</import>
	
	<!-- 
		EXPORT:
			Only users listed in this section will have content exported into the XML output file.
			All content from other users will be ignored.
			
			- To specify all users, enter a * wildcard
			- To list specific users, enter a CSV list of uids from the users section
			- To specify no users, leave it blank
			
			Example Scenario:  Export Communities for ONLY Dan Misawa and Frank Adams
			Example Configuration:  <communities>dmisawa,fadams</communities>
	 -->
	<export>
		<activities></activities>
		<blogs></blogs>
		<bookmarks></bookmarks>
		<communities></communities>
		<files></files>
		<forums></forums>
		<profiles></profiles>
		<wikis></wikis>
	</export>
	
	<!-- 
	*** WARNING! ALL IBM Connections content specified here will be deleted, not just content created by this application! ***
	
		DELETE:
			Only users listed in this section will have content deleted in IBM Connections.
			All content from other users will be ignored.
			
			- To specify all users, enter a * wildcard
			- To list specific users, enter a CSV list of uids from the users section
			- To specify no users, leave it blank
			
			Example Scenario:  Delete ALL Communities for ONLY Dan Misawa and Frank Adams
			Example Configuration:  <communities>dmisawa,fadams</communities>
			
	*** WARNING! ALL IBM Connections content specified here will be deleted, not just content created by this application! ***
	 -->
	<delete>
		<activities></activities>
		<blogs></blogs>
		<bookmarks></bookmarks>
		<communities></communities>
		<files></files>
		<forums></forums>
		<profiles></profiles>
		<wikis></wikis>
	</delete>
	
